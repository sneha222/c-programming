#include<stdio.h>
void input(int *a,int *b)
{
   scanf("%d",&a);
   scanf("%d",&b);

}
void compute(int a,int b,int *s)
{
 int s=0;
 *s=a+b;
}
void output(int s)
{
   printf("%d\n",s);
   }
int main()
{
   int a,b,x,s;
   printf("read a and b\n");
   input(x);
   compute(&a,&b);
   output(s);
   return 0;
}

