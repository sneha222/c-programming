#include<stdio.h>
int input()
{
   int x ;
   scanf("%d",&x);
   return x;
}
void compute(int *a,int*b)
{
 int s=0;
 s=*a+*b;
}
void output(int s)
{
   printf("%d\n",s);
}
int main()
{
   int a,b,x,s;
   printf("read a\n");
   a=input(x);
   printf("read b\n");
   b=input(x);
   compute(&a,&b);
   output(s);
   return 0;
}

